@model Exampler_ERP.Models.HR_EndOfService

<div class="modal fade" id="editEndOfServiceModal" tabindex="-1" role="dialog" aria-labelledby="editEndOfServiceModalLabel" aria-hidden="true" style="--bs-modal-width: 60rem;">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editEndOfServiceModalLabel">Edit End Of Service</h5>
                <button type="button" class="btn btn-danger CloseButton" data-dismiss="modal" id="Close">
                    x
                </button>
            </div>
            <div class="modal-body">
                <form id="editEndOfServiceForm" method="post" asp-action="Edit" asp-controller="EndOfService" enctype="multipart/form-data">
                    @Html.Partial("~/Views/HR/HR/EndOfService/_EditEndOfService.cshtml", Model)
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" id="Close">Close</button>
                <button type="button" class="btn btn-primary" id="saveEndOfServiceChanges">Save</button>
            </div>
        </div>
    </div>
</div>




